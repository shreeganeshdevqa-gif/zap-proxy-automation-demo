{
  "name": "cypress-bdd-proxy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test:chrome": "npx cypress run --browser chrome",
    "test:firefox": "cross-env CYPRESS_INTERNAL_BROWSER_CONNECT_TIMEOUT=60000 npx cypress run --browser firefox",
    "test:firefox:proxy": "cross-env HTTP_PROXY=http://localhost:8080 HTTPS_PROXY=http://localhost:8080 NO_PROXY=localhost,127.0.0.1 npm run test:firefox -- --headed",
    "report": "node generate-report.js"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cross-env": "^10.1.0",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^14.2.0",
    "dotenv": "^16.4.5"
  }
}